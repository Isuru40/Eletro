<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="firebase.mango.eletro">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity
            android:name=".SplashActivity"
            android:label="@string/app_name"
            android:theme="@style/SplashTheme">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".MainActivity" />
        <activity
            android:name=".Featurectivity"
            android:label="@string/title_activity_featurectivity"
            android:theme="@style/AppTheme.NoActionBar" />
        <activity android:name=".ACControlActivity"
            android:label="@string/ac_control"
            />
        <activity android:name=".Load_1_Activity"
            android:label="@string/load1_control"/>
        <activity android:name=".Load_2_Activity"
            android:label="@string/load2_control"/>
        <activity android:name=".Sensor_Activity"
            android:label="@string/sensor_control" />
        <activity android:name=".Device_info_Activity"
            android:label="@string/device_control"></activity>


    </application>

</manifest>